]>
<bird>
<predator>
<pred>
<name>Eagle</name>
<country>USA</country>
<lifeSpan>15 years</lifeSpan>
<size>large</size>
</pred>
<pred>
<name>Hawk</name>
<country>India</country>
<lifeSpan>20 years</lifeSpan>
<size>medium</size>
</pred>
<pred>
<name>Flamingo</name>
<country>India</country>
<lifeSpan>30 years</lifeSpan>
<size>large</size>
</pred>
</predator>
<prey>
<pre>
<name>Sparrow</name>
<country>India</country>
<lifeSpan>3 years</lifeSpan>
<size>small</size>
</pre>
<pre>
<name>Pigeon</name>
<country>India</country>
<lifeSpan>6 years</lifeSpan>
<size>medium</size>
</pre>
<pre>
<name>Myna</name>
<country>India</country>
<lifeSpan>4 years</lifeSpan>
<size>small</size>
</pre>
</prey>
</bird>